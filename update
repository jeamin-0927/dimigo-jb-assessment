sudo apt install git

cd /var/www/html

rm ~/vars.php
cp vars.php ~/vars.php

rm -rf ./*
rm -rf ./.*
git clone https://github.com/jeamin-0927/dimigo-jb-assessment .
git config --global --add safe.directory /var/www/html

if [ -f "~/vars.php" ]; then
	rm vars.php
	cp ~/vars.php vars.php
	echo "var copyed"
fi

sudo chmod 766 *
sudo chmod 777 update
